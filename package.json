{
  "name": "dhi",
  "version": "0.2.1",
  "author": "Rach Pradhan",
  "repository": {
    "type": "git",
    "url": "https://github.com/justrach/dhi.git"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.4",
    "wasm-pack": "^0.13.1",
    "zod": "^4.1.4"
  },
  "description": "High-performance TypeScript validation library with WebAssembly backend",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "validation",
    "schema",
    "types",
    "wasm",
    "webassembly",
    "performance"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:ts": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run test:wasm && npm run test:jest",
    "test:wasm": "cd rust && wasm-pack test --node",
    "test:jest": "node --experimental-vm-modules --no-warnings ./node_modules/jest/bin/jest.js",
    "benchmark": "bun run benchmarks/benchmark.ts",
    "benchmark:comprehensive": "bun run benchmarks/comprehensive.ts",
    "benchmark:real": "bun run benchmarks/realworld.ts",
    "benchmark:parallel": "node --experimental-modules --no-warnings --loader ts-node/esm benchmarks/parallel_nested.ts",
    "example": "bun run examples/example.ts",
    "publish": "npm run build && npm publish"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "ts-node": "^10.9.2"
  }
}
